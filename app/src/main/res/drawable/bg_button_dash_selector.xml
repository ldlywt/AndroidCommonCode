<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.5dp" android:color="@color/chrome_orange_54" android:dashWidth="@dimen/dp_4" android:dashGap="@dimen/dp_4" />
            <corners android:radius="@dimen/dp_8" />
            <solid android:color="@color/transparent" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:width="1.5dp" android:color="@color/gray_94" android:dashWidth="@dimen/dp_4" android:dashGap="@dimen/dp_4" />
            <corners android:radius="@dimen/dp_8" />
            <solid android:color="@color/gray_78" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1.5dp" android:color="@color/chrome_orange_74" android:dashWidth="@dimen/dp_4" android:dashGap="@dimen/dp_4" />
            <corners android:radius="@dimen/dp_8" />
            <solid android:color="@color/transparent" />
        </shape>
    </item>
</selector>
